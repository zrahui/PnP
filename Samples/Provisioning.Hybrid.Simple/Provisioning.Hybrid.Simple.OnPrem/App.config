<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <!-- Service Bus specific app settings for messaging connections -->
    <add key="ServiceBus.NameSpace" value="[Your Service Bus Namespace]" />
    <add key="ServiceBus.Secret" value="[Default key to service bus]" />
    <!-- 
      Provisioning settings for on-premises with service account model
      This assumes that you have enabled CSOM based site collection creation 
      in on premises using steps from following blog post:
      http://blogs.msdn.com/b/vesku/archive/2014/06/09/provisioning-site-collections-using-sp-app-model-in-on-premises-with-just-csom.aspx
    -->
    <add key="ProvisioningAccount" value="administrator"/>
    <add key="ProvisioningDomain" value="contoso"/>
    <add key="ProvisioningPassword" value="pass@word1"/>
    <add key="AdminSiteCollectionUrl" value="http://dev.contoso.com"/>
    <add key="LeadingURLForSiteCollections" value="http://dev.contoso.com"/>
  </appSettings>
</configuration>